#include<iostream>
using namespace std;

int main() {

    int age;
    cout << "Enter age of a user:";
    cin>>age;

    if (age >= 18) {
        cout << "You are eligible for voting \n";
        cout << "You are eligible for get driving licence";
    } else {
        cout << "You are not eligible for voting \n";
        cout << "you are not eligible for driving licence";
    }

    return 0;
}
